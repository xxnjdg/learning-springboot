<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.xxnjdg.learning.mybatis.helloworld.mapper.EmployeeMapper">
    <select id="queryEmployeeById" resultType="io.xxnjdg.learning.mybatis.helloworld.model.Employee">
    select id,last_name lastName,qender,email from tbl_employee where id = #{id}
  </select>
</mapper>